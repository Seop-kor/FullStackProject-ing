<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ImgBoardMapper">
    <select id="select" resultType="kr.co.seop.projecting.VO.ImgBoardVO">
        select * from imgboard
    </select>
    <select id="selectDetail" resultType="kr.co.seop.projecting.VO.ImgBoardVO">
        select * from imgboard where num = #{num}
    </select>
</mapper>